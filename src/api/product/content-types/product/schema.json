{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Produits",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "subtitle": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "richtext"
    },
    "content": {
      "type": "richtext"
    },
    "price": {
      "type": "integer"
    },
    "purchaseURL": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Atelier en ligne",
        "Atelier live",
        "Séjour",
        "Alliance"
      ],
      "default": "Atelier en ligne"
    },
    "isOpen": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "isDisplayed": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "discountPrice": {
      "type": "integer"
    },
    "hasDiscount": {
      "type": "boolean",
      "default": false
    },
    "imageOG": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "order": {
      "type": "integer"
    },
    "program": {
      "displayName": "program",
      "type": "component",
      "repeatable": true,
      "component": "product.program"
    },
    "countdown": {
      "displayName": "Compte à rebours",
      "type": "component",
      "repeatable": false,
      "component": "product.compte-a-rebours"
    },
    "picto": {
      "displayName": "Pictos",
      "type": "component",
      "repeatable": true,
      "component": "product.pictos"
    },
    "content2": {
      "type": "richtext"
    },
    "content3": {
      "type": "richtext"
    },
    "testimonial": {
      "displayName": "Témoignages",
      "type": "component",
      "repeatable": true,
      "component": "product.temoignages"
    },
    "content1": {
      "type": "richtext"
    },
    "preview": {
      "type": "component",
      "repeatable": false,
      "component": "layout.link"
    },
    "teaser": {
      "type": "component",
      "repeatable": false,
      "component": "layout.video-youtube"
    },
    "summary": {
      "type": "richtext"
    },
    "textprice": {
      "type": "string"
    },
    "isPro": {
      "type": "boolean",
      "default": false
    }
  }
}
